<FilesMatch \.p(hp|hp5|hp7|html)$>
    SetHandler proxy:unix:/opt/php8-3/var/run/php-fpm/labbio.sock|fcgi://labbio
</FilesMatch>

RewriteEngine On

# Excluir carpeta tools del rewrite
RewriteCond %{REQUEST_URI} ^/tools/
RewriteRule ^ - [L]


RewriteCond %{REQUEST_URI} !^/public/
RewriteRule ^(.*)$ public/$1 [L,QSA]



